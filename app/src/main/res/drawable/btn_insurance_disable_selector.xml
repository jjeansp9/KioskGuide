<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true" android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid
                android:color="@color/hospital_main_sel_color"
                />
            <stroke
                android:color="@color/hospital_sub_color"
                android:width="@dimen/hospital_insurance_bg_stroke_width"
                />
            <corners android:radius="8dp"/>
        </shape>
    </item>
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <solid
                android:color="@color/hospital_main_color"
                />
            <stroke
                android:color="@color/hospital_sub_color"
                android:width="@dimen/hospital_insurance_bg_stroke_width"
                />
            <corners android:radius="8dp"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid
                android:color="#80F7F7F7"
                />
            <stroke
                android:color="#80BBBBBB"
                android:width="@dimen/hospital_insurance_bg_stroke_width"
                />
            <corners android:radius="8dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid
                android:color="#80FFFFFF"
                />
            <stroke
                android:color="#80BBBBBB"
                android:width="@dimen/hospital_insurance_bg_stroke_width"
                />
            <corners android:radius="8dp"/>
        </shape>
    </item>
</selector>